package q11070;
class ConcatenateArrays{
	public void concatenate(int[] arr1,int[] arr2)
	{
	int n1=arr1.length;
	int n2=arr2.length;
	int[] arr3=new int[n1+n2];
	for(int i=0;i<n1;i++)
		{
			arr3[i]=arr1[i];
		}
	int j=0;
	for(int i=n1;i<arr3.length;i++)
		{
			arr3[i]=arr2[j];
			j++;
		}
	for(int i=0;i<arr3.length;i++)
		System.out.println(arr3[i]);
		
}
}
