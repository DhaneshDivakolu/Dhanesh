public class ElementCheck {
	
	/**
	 * Find if the first four elements in the array contains number 4 or not
	 * 
	 * 
	 * @return result
	 */
	 
	public boolean elementFinder(int[] arr) {
		boolean f=false;
		for(int i=0;i<arr.length;i++)
			{
				if(arr[i]!=4 || arr.length>4)
					f=false;
				else
					return true;
			}
		return f;
	}
}
