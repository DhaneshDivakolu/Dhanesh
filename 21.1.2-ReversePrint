package q11047;
public class ReversePrint {
	/**
	 * write a logic to find the array of elements in reverse order.
	 * 
	 * 
	 * @return the reverseArray 
	 */
	 
	 public int[] reversePrint(int[] arr) {
	 	int j=0;
		 int arr1[]=new int[arr.length];
		 for(int i=arr1.length-1;i>=0;i--)
			 {
				 arr1[j]=arr[i];
				 j++;
			 }
		 return arr1;
	 }
}
